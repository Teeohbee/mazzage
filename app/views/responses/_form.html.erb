<%= form_for @response do |f| %>
  <div class="form-group">
    <%= f.label :full_name %>
    <%= f.text_field :full_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :food_option %>
    <%= f.text_field :food_option, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :notes %>
    <%= f.text_area :notes, class: "form-control" %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
